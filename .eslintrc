{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [ "plugin:prettier/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-console": ["warn", { allow: ["warn", "error"] }],
    "quotes": ["error", "single"],
    "prettier/prettier": "warn",
    "react/prop-types": "off",
    "react/display-name": ["off"]
  }
}
